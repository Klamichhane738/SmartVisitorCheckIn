{
    "bindings": [
      {
        "authLevel": "function",
        "type": "httpTrigger",
        "direction": "in",
        "name": "req",
        "methods": [ "post" ]
      },
      {
        "type": "http",
        "direction": "out",
        "name": "res"
      },
      {
        "name": "outputDocument",
        "type": "cosmosDB",
        "direction": "out",
        "databaseName": "visitorDB",
        "collectionName": "checkins",
        "createDatabaseIfNotExists": true,
        "createCollectionIfNotExists": true,
        "connectionStringSetting": "CosmosDBConnection"
      }
    ]
  }
  